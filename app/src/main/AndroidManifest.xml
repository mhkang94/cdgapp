<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kang.cdg_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.kang.CDG_APP.make_account"
            android:label="@string/title_activity_make_account"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user_menu"
            android:label="@string/title_activity_user_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".info_change"
            android:label="@string/title_activity_info_change"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".recommend_food"
            android:label="@string/title_activity_recommend_food"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".sign_in"
            android:label="@string/title_activity_sign_in"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
